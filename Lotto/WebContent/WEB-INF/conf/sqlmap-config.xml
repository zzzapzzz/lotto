		<typeAlias alias="menuInfoDto" type="com.lotto.spring.domain.dto.MenuInfoDto"/>
		<typeAlias alias="winDataDto" type="com.lotto.spring.domain.dto.WinDataDto"/>
		<typeAlias alias="countSumDto" type="com.lotto.spring.domain.dto.CountSumDto"/>
